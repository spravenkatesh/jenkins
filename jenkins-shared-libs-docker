@Library("shared-libs") _

pipeline {
    agent {
        docker { image 'python:slim' }
    }
    stages {
        stage("hello-world") {
            steps {
                HelloWorld()
            }
        }
        stage("Hello parameterized") {
            steps {
                HelloParameterized("XYZ", "Tuesday")
            }
        }
        stage("Hello Mapping") {
            steps {
                HelloMap(name:"ABC", day:"Tuesday")
            }
        }
    }
}